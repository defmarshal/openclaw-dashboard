{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/data.json",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate, max-age=0",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
