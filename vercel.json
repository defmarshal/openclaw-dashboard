{
  "name": "openclaw-dashboard",
  "version": 2,
  "builds": [
    { "src": "static/.*", "use": "@vercel/static" },
    { "src": ".*", "use": "@vercel/static-build", "config": { "distDir": "." } }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
